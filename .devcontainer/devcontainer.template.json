{
	"name": "Python GUI Devcontainer - ${PLATFORM}",
	"build": {
		"dockerfile": "Dockerfile${DOCKERFILE_SUFFIX}"
	},
	"forwardPorts": [5901],
	"runArgs": ${RUN_ARGS},
	"mounts": ${MOUNTS},
	"overrideCommand": false,
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python"
			]
		}
	},
	"postCreateCommand": "${POST_CREATE_COMMAND}"
}